{
  "name": "@hoajs/cookie",
  "version": "0.1.0",
  "description": "cookie middleware for Hoa.",
  "main": "./dist/cjs/cookie.js",
  "type": "module",
  "module": "./dist/esm/cookie.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/cookie.js",
      "require": "./dist/cjs/cookie.js",
      "default": "./dist/esm/cookie.js"
    }
  },
  "files": [
    "dist",
    "types",
    "CHANGELOG.md"
  ],
  "scripts": {
    "lint": "eslint",
    "build": "tsup",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "prepare": "husky"
  },
  "author": "nswbmw",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoa-js/cookie.git"
  },
  "keywords": [
    "hoa",
    "middleware",
    "cookie"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.0.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/js": "^9.37.0",
    "eslint": "9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "16.4.0",
    "hoa": "0.1.0",
    "husky": "9.1.7",
    "jest": "30.1.3",
    "neostandard": "0.12.2",
    "prettier": "^3.6.2",
    "tsup": "8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "peerDependencies": {
    "hoa": "*"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "path-to-regexp": "8.3.0"
  }
}
